<div class= "sidenav">
    <% @categories.each do |category, dict|%>
      <%=link_to category.titleize, category_path(category), id: "category", remote: true%>
      <span >
      <% dict.each do |subcategory,subdict|%>
        <%= link_to subcategory.titleize, category_path(subcategory), id:"subcategory", remote: true%>
      <%end%>
      </span>
    <% end %>
  </div>
</div>
