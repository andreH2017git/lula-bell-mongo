
  <table>
    <tr>
      <th>Item ID</th>
      <td><%= f.text_field(:item_id, :value => "#{@item.id}", :readonly => true) %></td>
    </tr>
    <% if defined? @item.qty_id %>
    <tr>
      <th>Qty ID</th>
      <td><%= f.text_field(:qty_id, :value => "#{@item.qty_id}", :readonly => true) %></td>
    </tr>
    <% end %>
    <tr>
      <th>Student ID</th>
      <td><%= f.text_field(:student_id, :maxlength => 9) %></td>
    </tr>
    <tr>
      <th>Student Email</th>
      <td><%= f.text_field(:email) %></td>
    </tr>
    <tr>
      <th>Start Date</th>
      <td><%= f.date_field(:start_date, :order => [:month, :day, :year]) %></td>
    </tr>
    <tr>
      <th>End Date</th>
      <td><%= f.date_field(:end_date, :order => [:month, :day, :year]) %></td>
    </tr>
  </table>
